{
  "name": "kakko",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "changeset": "changeset",
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.2",
    "prettier": "^2.8.1",
    "turbo": "^1.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@6.11.0"
}
